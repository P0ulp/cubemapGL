uniform samplerCube cubemap;varying vec3 reflectDir;varying vec3 refractDir;varying float ratio;void main() {  	vec3 refle = vec3(reflectDir.x, -reflectDir.y, reflectDir.z);  	vec3 refra = vec3(refractDir.x, -refractDir.y, refractDir.z);	vec4 texRefle =  textureCube(cubemap, refle);	vec4 texRefra =  textureCube(cubemap, refra);	gl_FragColor  = mix(texRefra, texRefle, ratio);}